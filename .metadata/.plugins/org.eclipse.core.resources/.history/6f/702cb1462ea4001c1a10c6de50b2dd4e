package kh;

import java.util.Scanner;

public class op6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		float p=60, j=70, a=80, c=50, c2=30;
		float sc=p+j+a+c+c2;
		float m=sc/5;
		System.out.printf("나잘난의 성적 총합계는{%f}입니다. 평균은{%f}입니다",sc,m);
	
		int i=4, w=6, h=5;
		float v=i*w*h;
		System.out.printf("부피는%f입니다",v);
		
	}

}
