
public class control {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=9, b=15;
		if (a>=10)
		{
			System.out.println("a="+a);
		}
		else
		{
			a=0;
			System.out.println(a);
		}
		System.out.println("------");
		if (b>10)
		{
			System.out.println("b="+b);
		}
	}

}
