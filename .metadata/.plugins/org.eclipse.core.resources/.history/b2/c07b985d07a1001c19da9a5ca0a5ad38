package kh;

public class op3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x=10;
		int y=8;
		
		System.out.printf("%d을 %d로 나누면, \n",x,y);
		System.out.printf("몫은 %d이고,"
				+ "나머지는 %d입니다.\n", x/y,x%y);
		
		int A=54, B=66;
		System.out.println(A < B);
		
	}

}
